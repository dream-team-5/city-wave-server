<tr id="<%= dom_id wish %>">
  <td><%= wish.text.truncate 50, separator: ' ' %></td>

  <td><%= link_to 'View', [:admin, wish], remote: true, class: 'btn btn-outline-info btn-sm' %></td>

  <td><%= link_to 'Edit', [:edit, :admin, wish], remote: true, class: 'btn btn-outline-secondary btn-sm' %></td>

  <td><%= link_to 'Delete', [:admin, wish], remote: true, method: :delete, class: 'btn btn-outline-danger btn-sm' %></td>
</tr>
